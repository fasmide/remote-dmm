extends layout

block content
	div.row
		div.col-md-6#histogram
			div(style="height: 300px; width: 100%;")#histogramChartContainer
		div.col-md-6#reading
			p.value.text-right OL
			table.table.table-condensed
				thead
					tr
						th.text-center Vpp
						th.text-center Min
						th.text-center Max
						th.text-center Avg
						th.text-center Count
						th.text-center R/sec
				tr.text-center
					td.vpp
					td.min
					td.max
					td.avg
					td.count
					td.readssec
			p.text-right
				a(href="#").exportBtn Export
	div.row
		div.col-md-12#trend
			div(style="height: 300px; width: 100%;")#trendChartContainer